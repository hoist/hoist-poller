{
  "Hoist": {
    "application": {
      "name": "hoist-poller"
    },
    "rabbit": {
      "url": "amqp://rabbitmq",
      "managementurl": "http://guest:guest@rabbitmq:15672/"
    },
    "publisher": {
      "heartbeat": 60,
      "timeout": 120000
    },
    "mongo": {
      "core": {
        "connectionString": "mongodb://db/hoist-default"
      }
    },
    "filePaths": {
      "repositories": "/shares/repos",
      "deploys": "/shares/deploys",
      "connectors": "/shares/connectors"
    },
    "log": {
      "hub": true
    },
    "debug": true
  }
}
